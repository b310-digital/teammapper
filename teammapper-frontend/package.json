{
  "name": "teammapper-frontend",
  "version": "0.1.9",
  "description": "Web application to draw mind maps, based on mindmapp.",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/b310/teammapper.git"
  },
  "author": {
    "name": "b310 digital gmbh",
    "email": "kontakt@b310.de",
    "url": "https://b310.de"
  },
  "license": "MIT",
  "engines": {
    "npm": "~10.9.0",
    "node": "~22.12.0"
  },
  "keywords": [
    "mindmap",
    "node",
    "branch",
    "map",
    "mind",
    "infovis"
  ],
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host $BINDING --ssl false",
    "test": "ng test",
    "tsc": "npx tsc -p ./tsconfig.json --noEmit",
    "build:packages": "pnpm install --filter \"@teammapper/*\" && pnpm run --filter \"@teammapper/*\" build",
    "build:dev": "pnpm run build:packages && ng build",
    "build:prod": "pnpm run build:packages && ng build --configuration production",
    "lint": "ng lint",
    "doc": "compodoc -p src/tsconfig.app.json",
    "deploy": "ng deploy",
    "format": "npx prettier . --write"
  },
